{
	"name" : "hofff/contao-mod-template",
	"description" : "MetaPalettes extension for Contao OpenSource CMS",
	"keywords" : [
		"contao",
		"template"
	],
	"type" : "contao-module",
	"homepage" : "https://hofff.com",
	"license" : "LGPL-3.0+",
	"authors" : [{
			"name" : "Tristan Lins",
			"email" : "tristan.lins@bit3.de",
			"homepage" : "http://bit3.de",
			"role" : "Developer"
		},
		{
			"name" : "Oliver Hoff",
			"email" : "oliver@hofff.com",
			"homepage" : "https://hofff.com/",
			"role" : "Developer"
		},
	        {
	            "name": "Nicky Hoff",
	            "homepage": "https://hofff.com",
	            "email": "nick@hofff.com",
	            "role": "Manager"
	        }
	],
	"support" : {
		"email" : "info@hofff.com",
		"issues" : "https://github.com/hofff/contao-mod-template/issues",
		"wiki" : "https://github.com/hofff/contao-mod-template/wiki",
		"irc" : "irc://irc.freenode.org/contao.dev",
		"source" : "https://github.com/hofff/contao-mod-template"
	},
	"require" : {
		"php" : ">=5.3",
		"contao/core" : ">=2.11,<4",
		"contao-community-alliance/composer-installer" : "*",
		"bit3/contao-meta-palettes" : "~1.3"
	},
	"require-dev" : {
		"squizlabs/php_codesniffer" : "1.*",
		"bit3/php-coding-standard" : "@dev"
	},
	"replace" : {
		"contao-legacy/modtemplate" : "self.version"
	},
	"autoload" : {
		"psr-0" : {
			"Bit3" : "src/"
		}
	},
	"extra" : {
		"contao" : {
			"sources" : {
				"contao" : "system/modules/mod-template"
			}
		},
		"branch-alias" : {
			"dev-master" : "1.x-dev"
		}
	}
}
